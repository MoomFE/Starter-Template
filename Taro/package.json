{
  "private": true,
  "author": "Wei Zhang <https://github.com/Zhang-Wei-666>",
  "scripts": {
    "unocss:build": "unocss \"src/**/*.{js,ts,vue,html}\" --out-file src/styles/unocss.css",
    "unocss:dev": "npm run unocss:build -- --watch",
    "build-only:h5": "taro build --type h5",
    "build-only:rn": "taro build --type rn",
    "build-only:weapp": "taro build --type weapp",
    "dev-only:h5": "npm run build-only:h5 -- --watch",
    "dev-only:rn": "npm run build-only:rn -- --watch",
    "dev-only:weapp": "npm run build-only:weapp -- --watch",
    "build:h5": "npm run unocss:build && npm run build-only:h5",
    "build:rn": "npm run unocss:build && npm run build-only:rn",
    "build:weapp": "npm run unocss:build && npm run build-only:weapp",
    "dev:h5": "concurrently -n Taro,UnoCSS \"npm run dev-only:h5\" \"npm run unocss:dev\"",
    "dev:rn": "concurrently -n Taro,UnoCSS \"npm run dev-only:rn\" \"npm run unocss:dev\"",
    "dev:weapp": "concurrently -n Taro,UnoCSS \"npm run dev-only:weapp\" \"npm run unocss:dev\"",
    "build": "npm run build:h5",
    "dev": "npm run dev:h5",
    "lint": "eslint . --fix"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/components": "3.4.4",
    "@tarojs/plugin-framework-vue3": "3.4.4",
    "@tarojs/runtime": "3.4.4",
    "@tarojs/taro": "3.4.4",
    "pinia": "^2.0.6",
    "taro-plugin-pinia": "^1.0.0",
    "vue": "^3.2.24"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@moomfe/eslint-config": "^2.0.5",
    "@rollup/pluginutils": "^4.2.1",
    "@tarojs/cli": "^3.4.4",
    "@tarojs/mini-runner": "3.4.4",
    "@tarojs/webpack-runner": "3.4.4",
    "@types/webpack-env": "^1.13.6",
    "@unocss/cli": "^0.31.2",
    "babel-preset-taro": "3.4.4",
    "concurrently": "^7.1.0",
    "eslint": "^8.13.0",
    "eslint-config-taro": "3.4.4",
    "fs-extra": "^10.0.1",
    "typescript": "^4.1.0",
    "unocss": "^0.31.2",
    "vue-loader": "^16.8.3"
  }
}
